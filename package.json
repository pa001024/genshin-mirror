    "fast-json-stable-stringify": "^2.1.0",
    "fastify": "^3.11.0",
    "fastify-cookie": "^5.1.0",
    "fastify-jwt": "^2.3.0",
    "fastify-plugin": "^3.0.0",
    "fs-extra": "^9.1.0",
    "fuse.js": "^6.4.6",
